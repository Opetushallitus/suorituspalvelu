{
  "name": "suorituspalvelu-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "cd .. && husky",
    "dev": "next dev --turbopack",
    "build": "next build",
    "dev:test": "NEXT_PUBLIC_TEST=true npm run dev",
    "build:test": "NEXT_PUBLIC_TEST=true npm run build",
    "start": "npx serve out",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier -u -c *",
    "prettier:fix": "prettier --write -u *",
    "test": "vitest",
    "typecheck": "tsc",
    "test-playwright-docker": "./scripts/run-playwright-tests-in-docker.sh",
    "start-and-test-playwright-docker": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test start http://localhost:3000 'npm run test-playwright-docker -- $PLAYWRIGHT_ARGS'",
    "build-and-copy": "npm run build && mkdir -p ../suorituspalvelu-service/src/main/resources/static && cp -r out/suorituspalvelu/* ../suorituspalvelu-service/src/main/resources/static/"
  },
  "dependencies": {
    "@date-fns/tz": "^1.3.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^6.5.0",
    "@mui/material": "^6.5.0",
    "@opetushallitus/oph-design-system": "^1.0.0",
    "@tanstack/react-query": "^5.84.2",
    "@tolgee/format-icu": "^6.2.7",
    "@tolgee/react": "^6.2.7",
    "date-fns": "^4.1.0",
    "next": "15.4.6",
    "nuqs": "^2.4.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "remeda": "^2.30.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@types/node": "^22.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-playwright": "^2.2.2",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "serve": "^14.2.4",
    "start-server-and-test": "^2.0.13",
    "typescript": "^5",
    "typescript-eslint": "^8.39.0",
    "vitest": "^3.2.4"
  }
}
